<script type="text/javascript">
$(document).ready(function(){
	$('.web-directory-ul')
	.each(function(){
		  var max = 5
		  if ($(this).find("li").length > max) {
		    $(this)
		      .find('li:gt('+(max-1)+')')
		      .hide()
		      .end()
		      .append(
		        $('<li>More...</li>').click( function(){
		          $(this).siblings(':hidden').show().end().remove();
		        })
		    );
		  }
		});

	
	$('#web_directory_all').dataTable({
		"lengthMenu": [[-1, 10, 25, 50], ["All", 10, 25, 50]],
		"initComplete": function(settings, json) {
			$('#loading_message').hide();
			$('#web_directory_all').show();
		  },
		"columnDefs": [
                 {
                     "targets": [ 2 ],
                     "visible": false,
                     "searchable": true
                 },
                 {
                     "targets": [ 3 ],
                     "visible": false,
                     "searchable": false
                 },
                 {
                     "targets": [ 4 ],
                     "visible": false,
                     "searchable": false
                 }
          ],
          "drawCallback": function ( settings ) {
              var api = this.api();
              var rows = api.rows( {page:'current'} ).nodes();
              var last=null;
              var order = api.order();
              var sortc = 3;
              if(order[0][0] == 1){
				 sortc = 4;
              };
              api.column(sortc, {page:'current'} ).data().each( function ( group, i ) {
                  if ( last !== group ) {
                	  if(sortc == 3){
	                      $(rows).eq( i ).before(
	                          	'<tr class="group"><td><a name="group'+ group + '">'+group+'</a></td><td><span style="float:right"><a href="#listtop">top</a></span></td></tr>'
	                      );
                	  }
                	  else{
                		  $(rows).eq( i ).before(
  	                        	'<tr class="group"><td>&nbsp;</td><td><a name="group'+ group + '">'+group+'</a><span style="float:right"><a href="#listtop">top</a></span></td></tr>'
  	                      );
                	  }
   					  $(".anchor_links").append("<span style='padding:5px;'><a href='#group" + group + "'>" + group + "</a></span>");
                      last = group;
                  }
              } );
          }
	});
	$('#web_directory_all').on( 'length.dt', function ( e, settings, len ) {
	    //alert( 'New page length: '+len );
	    $(".anchor_links").html("");
	} );
	$('#web_directory_all').on( 'order.dt', function ( e, diff, edit ) {
		$(".anchor_links").html("");
	} );
	$('#web_directory_all').on( 'search.dt', function ( e, diff, edit ) {
		$(".anchor_links").html("");
	} );
});
</script>
